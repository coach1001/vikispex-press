{
  "machineSettings": {
    "type": "Dynamic Creep",
    "commPort": "COM11",
    "baudRate": 57600,
    "load": {
      "channels": [
        {
          "unit": "kN",
          "type": "input",
          "dataChannel": "adc2",
          "primary": true,
          "coffA": 0,
          "coffB": 0,
          "coffC": 0.06,
          "coffD": 0,
          "calc": "value*value*value*coffA+value*value*coffB+value*coffC+coffD",          
          "realMax": 60,
          "realMaxWarning": 55,
          "samples": 1,
          "realValuePrecision": 2
        }
      ]
    },
    "displacement": {
      "channels": [
        {
          "type": "input",
          "unit": "mm",
          "dataChannel": "adc2",
          "dataMin": 0,
          "dataMax": 1023,
          "primary": true,
          "length": 50,
          "calc": "(value/(dataMax-dataMin))*length",
          "realMin": 5,
          "realMinWarning": 15,
          "realMax": 45,
          "realMaxWarning": 40,
          "samples": 16,
          "realValuePrecision": 2
        }
      ]
    },
    "limit": {
      "channels": [
        {
          "type": "input",
          "unit": "integer",
          "dataChannel": "sw0",
          "activatedValue": 1
        },
        {
          "type": "input",
          "unit": "integer",
          "dataChannel": "sw1",
          "activatedValue": 1
        }
      ]
    },
    "motor": {
      "channels": [
        {
          "type": "output",
          "unit": "integer",
          "dataChannel": "pwm0",
          "primary": true,
          "outMin": 600,
          "outMax": 16000,
          "manual": 1000
        }
      ]
    },
    "direction": {
      "channels": [
        {
          "type": "output",
          "unit": "integer",
          "dataChannel": "r0",
          "advanceValue": 1,
          "reverseValue": 0,
          "idleValue": 0,
          "motorChannel": 0
        }
      ]
    }
  },
  "testTypes": [
    {
      "name": "RATE_OF_LOADING"
    },
    {
      "name": "RATE_OF_DISPLACEMENT"
    },
    {
      "name": "STATIC_CREEP"
    },
    {
      "name": "DYNAMIC_CREEP",
      "initialState": {
        "state": "DYNAMIC_CREEP",
        "subState": "ZERO_LOAD"
      }
    }
  ],
  "tests": [
    {
      "id": 1,
      "name": "Dynamic Creep",
      "testType": "DYNAMIC_CREEP",
      "constants": [
        {
          "name": "advanceSpeed",
          "value": 1000
        },
        {
          "name": "loadDetectRealValue",
          "value": 0.5
        },
        {
          "name": "motorLoadSpeed",
          "value": 15000
        }
      ],
      "calculated": [
        {
          "name": "load",
          "calc": "(((diameter/2)*(diameter/2)*pi)/1000)*stress",
          "precision": 2
        },
        {
          "name": "cycleTime",
          "calc": "(1/frequency)",
          "precision": 5
        }
      ],
      "inputs": [
        {
          "fieldType": "TextInput",
          "label": "Sample number",
          "name": "sampleNumber",
          "validation": "required",
          "defaultValue": "Default"
        },
        {
          "fieldType": "DatePickerInput",
          "label": "Date",
          "name": "date",
          "validation": "required"
        },
        {
          "fieldType": "NumberInput",
          "label": "Conditioning cycles",
          "name": "conditioningCycles",
          "validation": "required|min_value:1|numeric|max_value:200",
          "defaultValue": 5
        },
        {
          "fieldType": "NumberInput",
          "label": "Cycles",
          "name": "cycles",
          "validation": "required|min_value:1|numeric|max_value:999999",
          "defaultValue": 5
        },
        {
          "fieldType": "NumberInput",
          "label": "Stress (kPa)",
          "name": "stress",
          "validation": "required|min_value:1|decimal:1|max_value:200",
          "defaultValue": 20
        },
        {
          "fieldType": "NumberInput",
          "label": "Diameter (mm)",
          "name": "diameter",
          "validation": "required|min_value:10|decimal:1|max_value:500",
          "defaultValue": 40
        },
        {
          "fieldType": "NumberInput",
          "label": "Frequency (Hz)",
          "name": "frequency",
          "validation": "required|min_value:0.01|decimal:2|max_value:4",
          "defaultValue": 0.5
        },
        {
          "fieldType": "NumberInput",
          "label": "Sample height (mm)",
          "name": "sampleHeight",
          "validation": "required|min_value:20|decimal:1|max_value:300",
          "defaultValue": 60
        }
      ]
    }
  ]
}